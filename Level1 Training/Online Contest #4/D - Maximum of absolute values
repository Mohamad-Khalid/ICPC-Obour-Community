#include <iostream>
#include <iomanip>

using namespace std;

int main(){
  

  int n ;
  double MAX=0;
  cin >> n;
  for(int i=0;i<n;i++){
    double x;
    cin >> x;
    if(x<0){
      x=x*-1; //x*=1
    }

    if(x>MAX){
      MAX = x;
    }
  }

  cout << fixed << setprecision(2) << MAX;

  return 0;
} 

#include <iostream>
#include <iomanip>

using namespace std;

double A[105];

int main(){
  

  int n ;
  cin >> n;
  for(int i=0;i<n;i++){
   cin >> A[i];
  }

  for(int i=0;i<n;i++){
    if(A[i]<=2.5){
      cout << i+1 <<" "<< fixed << setprecision(2) << A[i];
      return 0;
    }
  }

  cout <<"Not Found";


  return 0;
} 
